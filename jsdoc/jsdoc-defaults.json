{
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "default": {
      "staticFiles": {
        "include": [
          "./jsdoc/static"
        ]
      }
    }
  },
  "opts": {
    "destination": "./docs",
    "template": "./jsdoc",
    "layoutFrags": {
      "head": "./jsdoc/tmpl/head.tmpl",
      "nav": "./jsdoc/tmpl/nav.tmpl",
      "foot": "./jsdoc/tmpl/foot.tmpl"
    },
    "deploy": {
      "pages": {
        "branch": "gh-pages",
        "cloneToPath": "publish",
        "user": {
          "name": "${package.author.name}",
          "email": "${package.author.email}"
        }
      }
    },
    "pages": {
      "menu": {
        "logo": "",
        "package": "",
        "changelog": "",
        "source": "",
        "style": ""
      },
      "links": [],
      "meta": [],
      "scripts": []
    },
    "versions": {
      "from": "",
      "include": "minor"
    },
    "changelog": {
      "header": "## [${package.version}](${publish.moduleURL}/compare/v${publish.lastVersion}...v${package.version}) (${publish.date})",
      "line": "* [%s](${publish.moduleURL}/commit/%H)",
      "sections": {
        "breaks": {
          "grep": {
            "regexp": "\\[BREAK\\]",
            "ignoreCase": true,
            "extendedRegexp": false,
            "allMatch": false
          },
          "header": "__Breaking Changes:__"
        },
        "features": {
          "grep": {
            "regexp": "\\[FEATURE\\]",
            "ignoreCase": true,
            "extendedRegexp": false,
            "allMatch": false
          },
          "header": "__Features:__"
        },
        "fixes": {
          "grep": {
            "regexp": "\\[FIX\\]",
            "ignoreCase": true,
            "extendedRegexp": false,
            "allMatch": false
          },
          "header": "__Fixes:__"
        }
      }
    }
  }
}