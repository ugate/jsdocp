{
  "name": "jsdocp",
  "version": "0.2.2",
  "description": "Simplified JSDoc publishing using git",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Tests ran during deployment...\"",
    "jsdocp": "jsdocp ./jsdocp/jsdoc.json",
    "jsdocp-deploy": "npm link && jsdocp -D ./jsdocp/jsdoc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ugate/jsdocp.git"
  },
  "keywords": [
    "jsdoc",
    "documentation",
    "publish",
    "git",
    "gh-pages",
    "continuous-integration",
    "travis-ci"
  ],
  "author": "ugate",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ugate/jsdocp/issues"
  },
  "homepage": "https://github.com/ugate/jsdocp#readme",
  "dependencies": {
    "jsdoc": "^3.5.5"
  },
  "devDependencies": {
    "docdash": "^1.0.0",
    "ink-docstrap": "^1.3.2",
    "jaguarjs-jsdoc": "^1.1.0",
    "minami": "^1.2.3"
  },
  "bin": "./cli.js",
  "files": [
    "/index.js",
    "/cli.js",
    "/deploy/**/*",
    "/jsdoc/**/*"
  ]
}
