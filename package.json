{
  "name": "jspub",
  "version": "1.0.0",
  "description": "Simplified JSDoc publishing using git",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Tests ran during deployment...\"",
    "jspub": "jspub ./jspub/jsdoc.json",
    "jspub-deploy": "jspub-deploy ./jspub/jsdoc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ugate/jspub.git"
  },
  "keywords": [
    "jsdoc",
    "documentation",
    "publish",
    "git",
    "gh-pages",
    "continuous-integration",
    "travis-ci"
  ],
  "author": "ugate",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ugate/jspub/issues"
  },
  "homepage": "https://github.com/ugate/jspub#readme",
  "dependencies": {
    "jsdoc": "^3.5.5"
  },
  "devDependencies": {
    "code": "^5.2.0",
    "lab": "^16.1.0",
    "minami": "^1.2.3"
  },
  "bin": {
    "jspub": "./cli.js",
    "jspub-deploy": "./deploy/pages.js"
  },
  "files": [
    "/index.js",
    "/cli.js",
    "/deploy/**/*",
    "/jsdoc/**/*"
  ]
}
