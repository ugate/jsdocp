{
  "source": {
    "include": ["index.js", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "showInheritedInNav": true,
    "default": {
      "useLongnameInNav": false,
      "includeDate": false,
      "staticFiles": {
        "include": [
          "./jsdoc/jsdoc-defaults.json",
          "./jspub/jsdoc.json"
        ]
      }
    }
  },
  "opts": {
    "template": "./node_modules/minami",
    "tutorials": "./jspub/tutorial",
    "encoding": "utf8",
    "private": false,
    "recurse": true,
    "deploy": {
      "pages": {
        "user": {
          "name": "Travis",
          "email": "travis@travis-ci.org"
        }
      }
    },
    "versions": {
      "from": "1.0.0",
      "include": ""
    }
  }
}